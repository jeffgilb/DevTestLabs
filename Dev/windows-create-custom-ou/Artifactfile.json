{
    "$schema": "https://raw.githubusercontent.com/Azure/azure-devtestlab/master/schemas/2016-11-28/dtlArtifacts.json",
    "title": "Create custom OU",
    "description": "Use this to create a custom organizational unit.",
    "publisher": "JeffGilb",
    "tags": [
        "AD DS"
    ],
    "iconUri": "https://raw.githubusercontent.com/jeffgilb/devtestlabs/master/Dev/jeff.png",
    "targetOsType": "Windows",
    "parameters": {
        "OUname": {
            "type": "string",
            "displayName": "OU Name",
            "description": "New OU name."
        },
        "OUdescription": {
            "type": "string",
            "displayName": "OU Description",
            "description": "New OU description.",
            "allowEmpty": true
        }
    },
    "runcommand": {
            "commandToExecute": "['powershell.exe -executionpolicy bypass -File createou.ps1, ' ', parameters('OUname'), ' ', parameters('OUdescription'))]"
        }
    }
}
