{
    "$schema": "https://raw.githubusercontent.com/Azure/azure-devtestlab/master/schemas/2016-11-28/dtlArtifacts.json",
    "title": "Create OU",
    "description": "Create a custom organizational units in AD DS.",
    "publisher": "JeffGilb",
    "tags": [
        "Windows"
    ],
    "iconUri": "https://raw.githubusercontent.com/jeffgilb/devtestlabs/master/Dev/jeff.png",
    "targetOsType": "Windows",
    "parameters": {
        "OUname": {
            "type": "string",
            "displayName": "OU Name",
            "description": "Display name for new organizational unit."
        },
        "OUdescription": {
            "type": "string",
            "displayName": "OU Description",
            "description": "Description for new organizational unit.",
            "allowEmpty": true
    }
      "runCommand" {
          "commandToExecute": "[concat('powershell.exe -ExecutionPolicy bypass \"& ./createou.ps1', ' ', parameters('OUname'), ' ', parameters('OUdescription'))]"
    }

}
